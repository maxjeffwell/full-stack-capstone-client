{
  "name": "educationelly-client",
  "version": "1.0.0",
  "author": "Jeff Maxwell <maxjeffwell@gmail.com>",
  "license": "GNU GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "history": "^5.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-modal": "^3.16.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.1",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "styled-components": "^6.1.13",
    "web-vitals": "^5.0.3",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:ci": "react-scripts test --env=jsdom --coverage --watchAll=false --ci",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,scss,md}",
    "type-check": "echo 'No TypeScript in this project'",
    "validate": "npm run lint && npm run format:check && npm run test:ci",
    "commit": "cz",
    "deps:check": "depcheck",
    "deps:update": "npx npm-check-updates",
    "deps:upgrade": "npx npm-check-updates -u && npm install",
    "audit": "npm audit",
    "audit:fix": "npm audit fix --force",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "analyze": "npm run build && npx serve -s build",
    "check-tools": "node scripts/check-tools.js",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "babel-plugin-styled-components": "^2.1.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.6.2"
  },
  "heroku-run-build-script": true,
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|msw|@bundled-es-modules)/)"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
